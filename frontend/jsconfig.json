{
    "compilerOptions": {
        "baseUrl": "src",
        "paths": {
            "@API/*": [
                "API/*"
            ],
            "@data/*": [
                "data/*"
            ],
            "@hooks/*": [
                "hooks/*"
            ],
            "@images/*": [
                "images/*"
            ],
            "@modules/*": [
                "modules/*"
            ],
            "@pages/*": [
                "pages/*"
            ]
        }
    },
    "exclude": [
        "node_modules"
    ]
}